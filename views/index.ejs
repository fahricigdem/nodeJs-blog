<!DOCTYPE html>
<html lang="en">
<head>
    <%-include('./inc/head')%>
</head>
<body>
    <%-include('./inc/nav')%>
    <section>
        <h1>Blogs</h1>
        <ul>
            <%blogs.forEach((blog)=>{%>
            <li>
                <a href="/blog/<%=blog._id%>">
                    <%=blog.title%>
                </a>
                <p><%=blog.short%></p>
            </li>
            <%})%>
        </ul>
    </section>
    
</body>
</html>